{
  "name": "@michitaro/react-menu",
  "version": "1.0.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-example": "vite build --config example.vite.config.ts",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx",
    "typedoc": "typedoc --out ./github-pages/docs --media ./media ./src/react-menu/index.ts",
    "gh-pages": "npm run build-example && npm run typedoc && gh-pages -d ./github-pages",
    "test": "vitest"
  },
  "files": [
    "/dist",
    "/types/react-menu"
  ],
  "types": "./types/react-menu/index.d.ts",
  "main": "./dist/react-menu.cjs.js",
  "module": "./dist/react-menu.es.js",
  "exports": {
    "import": "./dist/react-menu.es.js",
    "require": "./dist/react-menu.cjs.js"
  },
  "type": "module",
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "dependencies": {
    "aphrodite": "^2.4.0",
    "classnames": "^2.3.1",
    "react-transition-group": "^4.4.2"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.3",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-react": "^1.0.7",
    "eslint": "^8.8.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^3.2.3",
    "happy-dom": "^2.39.1",
    "react-router-dom": "^6.2.2",
    "typedoc": "^0.22.11",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vitest": "^0.4.2"
  }
}
